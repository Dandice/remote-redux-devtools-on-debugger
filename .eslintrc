{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "consistent-return": 0,
    "strict": 0,
    "no-console": 0,
    "no-underscore-dangle": ["error", { "allow": ["__DEV__"] }]
  }
}
