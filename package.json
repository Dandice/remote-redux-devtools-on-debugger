{
  "name": "remote-redux-devtools-on-debugger",
  "version": "0.3.0",
  "description": "The remote-redux-devtools / remotedev monitor on react-native debugger",
  "main": "bin/debugger.js",
  "files": [
    "bin",
    "bundle.js"
  ],
  "bin": {
    "remotedev-debugger-replace": "bin/cli.js",
    "remotedev-debugger": "bin/cli.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true,
      "node": true
    },
    "rules": {
      "consistent-return": 0,
      "strict": 0,
      "prefer-template": 0
    }
  },
  "scripts": {
    "lint": "eslint bin/*.js app/*.js webpack.*",
    "clean": "rm bundle.js debugger.html",
    "build:webpack": "webpack --stats --progress",
    "build": "npm run build:webpack",
    "dev": "webpack-dev-server --config=webpack.config.dev.js --host=localhost --hot --inline --colors",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jhen0409/remote-redux-devtools-on-debugger.git"
  },
  "keywords": [
    "remote",
    "redux",
    "devtools",
    "remotedev",
    "react",
    "native",
    "desktop",
    "debugger"
  ],
  "authors": [
    "Jhen <developer@jhen.me> (https://github.com/jhen0409)",
    "Mihail Diordiev <zalmoxisus@gmail.com> (https://github.com/zalmoxisus)"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "eslint": "~2.2.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-react": "^4.2.3",
    "react": "^0.14.7",
    "react-dock": "^0.2.3",
    "react-dom": "^0.14.7",
    "remotedev-app": "^0.2.8",
    "webpack": "^1.12.10",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "minimist": "^1.2.0",
    "remotedev-server": "0.0.4"
  }
}
